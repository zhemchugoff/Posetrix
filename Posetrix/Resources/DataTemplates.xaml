<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:core="clr-namespace:Posetrix.Core.ViewModels;assembly=Posetrix.Core"
    xmlns:views="clr-namespace:Posetrix.Views">
    <DataTemplate DataType="{x:Type core:CustomIntervalViewModel}">
        <views:CustomIntervalView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type core:PredefinedIntervalsViewModel}">
        <views:PredefinedIntervalsView />
    </DataTemplate>
</ResourceDictionary>